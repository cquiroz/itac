            <artifactId>qengine-api</artifactId>
            <version>2017.2.1-SNAPSHOT</version>
        </dependency>
        <dependency>
            <groupId>edu.gemini.tac</groupId>
            <artifactId>qengine-log</artifactId>
            <version>2017.2.1-SNAPSHOT</version>
        </dependency>
        <dependency>
            <groupId>joda-time</groupId>
